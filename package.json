{
  "name": "complinist-desktop",
  "private": true,
  "version": "1.4.0",
  "description": "NIST Compliance and System Security Planning Tool",
  "author": {
    "name": "CompliNist",
    "email": "support@complinist.com"
  },
  "license": "AGPL-3.0",
  "type": "module",
  "main": "electron/main.js",
  "homepage": "./",
  "scripts": {
    "dev": "vite",
    "server": "node server/index.js",
    "server:dev": "node --watch server/index.js",
    "server:rebuild": "npm rebuild better-sqlite3",
    "docker:build": "docker build -t compliflow .",
    "docker:run": "docker run -p 3000:3000 -v ./models:/data/models compliflow",
    "docker:compose": "docker-compose up -d",
    "build": "vite build",
    "build:fast": "node scripts/build/fast-build.js build",
    "build:incremental": "node scripts/build/fast-build.js incremental",
    "build:full": "node scripts/build/fast-build.js full",
    "build:watch": "node scripts/build/fast-build.js watch",
    "build:clean": "node scripts/build/fast-build.js clean",
    "build:status": "node scripts/build/fast-build.js status",
    "cache:status": "node scripts/build/build-cache.js status",
    "cache:clear": "node scripts/build/build-cache.js clear",
    "cache:changes": "node scripts/build/build-cache.js changes",
    "typecheck": "tsc --noEmit --incremental",
    "typecheck:watch": "tsc --noEmit --incremental --watch",
    "preview": "vite preview",
    "postinstall": "electron-builder install-app-deps",
    "electron:dev": "npm run build && (vite build --watch &) && wait-on file:dist/index.html --interval 100 --delay 500 && sleep 1 && electron . --no-sandbox",
    "electron:dev:hmr": "concurrently -k -n \"VITE,ELECTRON\" -c \"cyan,yellow\" \"npm run dev\" \"wait-on http://localhost:5173 && NODE_ENV=development ELECTRON_HMR=true VITE_DEV_SERVER_URL=http://localhost:5173 electron . --no-sandbox\"",
    "dev:watch": "concurrently -k -n \"VITE,ELECTRON\" -c \"cyan,yellow\" \"npm run dev\" \"wait-on http://localhost:5173 && NODE_ENV=development ELECTRON_HMR=true ELECTRON_RELOADER=true VITE_DEV_SERVER_URL=http://localhost:5173 electron . --no-sandbox\"",
    "electron:dev:fast": "node scripts/build/fast-build.js electron:dev",
    "electron:rebuild": "npx @electron/rebuild -f -w better-sqlite3",
    "electron:rebuild:win": "npx prebuild-install --platform=win32 --arch=x64 -r electron -d node_modules/better-sqlite3 || echo 'Prebuild not available, will use electron-builder rebuild'",
    "electron:build": "npm run electron:rebuild && npm run build && electron-builder",
    "electron:build:win": "npm run build && electron-builder --win --x64",
    "electron:build:win:native": "npm run electron:rebuild && npm run build && electron-builder --win",
    "electron:build:linux": "npm run electron:rebuild && npm run build && electron-builder --linux",
    "electron:build:all": "npm run build && electron-builder --win --linux",
    "create-icons": "node scripts/utils/create-icons.js",
    "build-release": "./scripts/build/build-release.sh",
    "release:package": "node scripts/build/create-release-archive.js",
    "release:package:linux": "node scripts/build/create-release-archive.js --platform=linux",
    "release:package:win": "node scripts/build/create-release-archive.js --platform=win",
    "package-data": "./scripts/utils/package-data.sh",
    "upload-data": "./scripts/utils/upload-data-to-s3.sh",
    "upload-installers": "./scripts/utils/upload-installers-to-s3.sh",
    "start": "./scripts/dev/launch.sh start",
    "stop": "./scripts/dev/launch.sh stop",
    "test:license": "node tests/e2e/license/test-license-import.mjs",
    "test:license:watch": "node --watch tests/e2e/license/test-license-import.mjs",
    "test:license:cancel": "node tests/e2e/license/test-cancel-scenario.mjs",
    "test:license:persistence": "node tests/e2e/license/test-persistence.mjs",
    "test:license:all": "npm run test:license && npm run test:license:cancel && npm run test:license:persistence",
    "test": "vitest",
    "test:ui": "vitest --ui --open",
    "test:ui:coverage": "vitest --ui --coverage --open",
    "test:ui:stores": "vitest --ui --open tests/unit/stores",
    "test:ui:controls": "vitest --ui --open tests/unit/controls",
    "test:ui:terraform": "vitest --ui --open tests/unit/terraform",
    "test:watch": "vitest --watch",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage",
    "test:coverage:ui": "vitest --coverage --ui --open",
    "test:changed": "vitest --changed",
    "test:debug": "vitest --ui --inspect-brk --open",
    "test:cursor": "bash scripts/test-cursor-modes.sh --all",
    "test:cursor:unit": "vitest run tests/unit/stores/cursorModes.test.ts",
    "test:cursor:e2e": "playwright test tests/e2e/boundary-cursor-modes.spec.ts --config=playwright.cursor-tests.config.ts",
    "test:cursor:visual": "playwright test tests/visual/cursor-states.spec.ts --config=playwright.cursor-tests.config.ts",
    "test:cursor:visual:update": "playwright test tests/visual/cursor-states.spec.ts --config=playwright.cursor-tests.config.ts --update-snapshots",
    "ai:cli": "node scripts/utils/ai-test.js",
    "ai:query": "node scripts/utils/ai-test.js query",
    "ai:health": "node scripts/utils/ai-test.js health",
    "test:ai:schema": "node scripts/test/test-ai-embedding-fix.js",
    "test:ai:integration": "node scripts/test/test-ai-embedding-integration.js",
    "test:ai:e2e": "playwright test tests/e2e/ai-embedding.spec.js",
    "test:ai:diagnose": "node scripts/test/diagnose-ai-embedding.js",
    "test:ai:chromadb": "node scripts/test/test-chromadb-filters.js",
    "test:terraform:screenshot": "playwright test tests/e2e/terraform-import-screenshot.spec.ts --config=tests/e2e/playwright.config.ts --project=terraform-screenshot",
    "ingest:list": "python3 scripts/ingestion/ingest_compliance_docs.py --list",
    "ingest:file": "python3 scripts/ingestion/ingest_compliance_docs.py --file",
    "ingest:dir": "python3 scripts/ingestion/ingest_compliance_docs.py --dir",
    "ingest:800-53": "python3 scripts/ingestion/ingest_nist_800_53.py",
    "lint": "eslint src --max-warnings 0",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\"",
    "layout:templates": "tsx scripts/layout-templates.ts",
    "layout:templates:apply": "tsx scripts/apply-template-layout.ts"
  },
  "dependencies": {
    "@dagrejs/dagre": "^1.1.8",
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.1",
    "@rjsf/core": "^5.24.13",
    "@rjsf/utils": "^5.24.13",
    "@rjsf/validator-ajv8": "^5.24.13",
    "@tisoap/react-flow-smart-edge": "^4.0.1",
    "@trpc/client": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "@types/react-window": "^1.8.8",
    "@xyflow/react": "^12.3.5",
    "better-sqlite3": "^11.7.0",
    "chromadb": "^1.10.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.6",
    "electron-trpc": "^0.7.1",
    "elkjs": "^0.11.0",
    "express": "^4.22.1",
    "file-saver": "^2.0.5",
    "html-to-image": "^1.11.13",
    "jose": "^6.1.3",
    "lucide-react": "^0.468.0",
    "multer": "^1.4.5-lts.1",
    "node-llama-cpp": "^3.14.2",
    "papaparse": "^5.5.3",
    "potpack": "^2.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-window": "^2.2.4",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.1.13",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.1",
    "@eslint/js": "^9.17.0",
    "@jest/globals": "^30.2.0",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/dagre": "^0.7.52",
    "@types/file-saver": "^2.0.7",
    "@types/jest": "^30.0.0",
    "@types/papaparse": "^5.5.1",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/ui": "^4.0.16",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "electron": "^33.4.11",
    "electron-builder": "^25.1.8",
    "electron-rebuild": "^3.2.9",
    "electron-reloader": "^1.2.3",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "jest": "^30.2.0",
    "jsdom": "^27.4.0",
    "playwright": "^1.57.0",
    "png-to-ico": "^3.0.1",
    "postcss": "^8.4.49",
    "prettier": "^3.7.4",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.4.6",
    "tsx": "^4.20.6",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^5.4.11",
    "vitest": "^4.0.16",
    "wait-on": "^8.0.1"
  }
}
